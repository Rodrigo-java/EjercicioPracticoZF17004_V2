                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT
    COALESCE(s.PromotionCode,'')       AS PromotionCode,
    COALESCE(p.KitType,'')             AS KitType,
    COALESCE(p.Channels, 0)            AS Channels,
    COALESCE(p.Demographic,'')         AS Demographic,
    COALESCE(p.ItemGroup,'')           AS ItemGroup
