                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    s.OrderNumber,
    s.OrderDate,
    s.ShipDate,
    s.CustomerStateID,
    s.ProductID,
    p.KitType,
p.Channels,
p.Demographic,
  p.ItemGroup,
    s.Quantity,
    s.UnitPrice,
    s.DiscountAmount,
    s.PromotionCode,
    so.SalesOfficeID,
    so.StateID,
    st.RegionID,
    r.RegionName
FROM Sales AS s
LEFT JOIN Product AS p
    ON s.ProductID = p.ProductID
LEFT JOIN SalesOffice AS so
    ON so.StateID = s.CustomerStateID
LEFT JOIN State AS st
    ON st.StateID = s.CustomerStateID
LEFT JOIN Region AS r
    ON r.RegionID = st.RegionID;</property>
